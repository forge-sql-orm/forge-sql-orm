{
  "name": "jira-global-page-ui-kit",
  "version": "1.1.33",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "knip": "knip",
    "models:create": "forge-sql-orm-cli generate:model --output src/database/entities --saveEnv",
    "migration:create": "forge-sql-orm-cli migrations:create --force --output src/database/migration",
    "migration:update": "forge-sql-orm-cli migrations:update --entitiesPath src/database/entities --output src/migration",
    "build:ci": "tsc --target esnext --project tsconfig.json  --outDir dist && rm -rf dist"
  },
  "devDependencies": {
    "eslint-plugin-react-hooks": "^7.0.1",
    "forge-sql-orm-cli": "^2.1.24",
    "knip": "^5.84.0"
  },
  "dependencies": {
    "@forge/bridge": "5.13.0",
    "@forge/react": "11.12.0",
    "@forge/resolver": "1.7.1",
    "@forge/sql": "^3.0.18",
    "drizzle-orm": "^0.45.1",
    "forge-sql-orm": "^2.1.24",
    "react": "^18.2.0"
  }
}
